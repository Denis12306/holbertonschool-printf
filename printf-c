#include <stdio.h>
#include <main.h>
#include <stdarg.h>
/**
 * printf - a function that print an input format
 * format is a character string
 * Return : the number of characters printed
 */
int _printf(const char *format, ...)
{
	int i;

	for (i <= 'a'; i >= 'z'; i++)
	{
		_putchar(i);
	}
	for (i <= 'A'; i >= 'Z'; i++)
	{
		_putchar(i);
	}
	return(format[i]);
}